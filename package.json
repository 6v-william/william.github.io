{
  "name": "pc-template",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "umi dev",
    "start:prod": "DEBUG_ENV=prod umi dev",
    "build": "rm -rf node_modules/ && DEBUG_ENV=prod umi build",
    "build:test": "rm -rf dist/ && DEBUG_ENV=test umi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "clean": "rm -rf node_modules **/*/node_modules",
    "lint": "eslint ."
  },
  "watch": {
    "build": "{src}/*"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@flc/workbench-tools": "^0.0.63",
    "ahooks": "^3.7.8",
    "antd": "^5.7.0",
    "dayjs": "^1.11.10",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.15",
    "@types/react-i18next": "^8.1.0",
    "less-loader": "^12.2.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "umi": "^4.0.83",
    "yorkie": "^2.0.0"
  },
  "author": "迦叶 <lw_murphy@163.com>"
}
